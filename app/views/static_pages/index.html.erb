<h1>Enter User ID</h1>
<%= form_with url: '/', method: :get do |form| %>
  <%= form.label :user_id, 'User ID: ' %>
  <%= form.text_field :user_id ,:required => true %>
  <%= form.submit :submit %>
<% end %> 
<% if @flickr_response %>
  <% @flickr_response.each do |item| %>
  <%= item['id']%>
  <% end %>
<% end %>